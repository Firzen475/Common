config global
        option anon_swap '0'
        option anon_mount '0'
        option auto_swap '1'
        option auto_mount '1'
        option delay_root '5'
        option check_fs '0'

config mount
        option device '/dev/sda2'
        option target '/mnt/tmp_overlay'
        option label 'MMC_256MB'
        option fstype 'ext4'
        option is_rootfs '1'
        option options 'rw,noatime'
        option delay_root '13'
        option enabled '1'

config mount
        option device '/dev/sda3'
        option target '/mnt/nas'
        option fstype 'ext4'
        option options 'rw,noatime'
        option enabled '1'

config swap
        option device '/dev/sda1'
        option enabled '1'
